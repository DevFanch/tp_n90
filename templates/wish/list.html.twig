{% extends 'base.html.twig' %}

{% block title %}Hello WishController!{% endblock %}

{% block body %}
<h1>{{title}}</h1>
<section>
	<ul>
        {% for wish in wishes %}
    		<li><a href="{{ path('wish_detail', {'id': wish.id}) }}">{{wish.title}}</a></li>
        {% endfor %}
	</ul>
</section>

{% endblock %}
